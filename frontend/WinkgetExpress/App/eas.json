{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "APP_ENV": "production"
      },
      "autoIncrement": true
    },
    "production-apk": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE": "https://captain-winkget-express.vercel.app",
        "EXPO_PUBLIC_SOCKET_URL": "https://captain-winkget-express.vercel.app"
      },
      "autoIncrement": true
    },
    "preview-apk": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE": "https://captain-winkget-express.vercel.app",
        "EXPO_PUBLIC_SOCKET_URL": "https://captain-winkget-express.vercel.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
